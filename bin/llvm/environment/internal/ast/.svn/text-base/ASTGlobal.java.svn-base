package llvm.environment.internal.ast;

public final class ASTGlobal extends ASTConstantExpression {

	private final String name;
	
	public ASTGlobal(String name) {
		this.name = name;
	}
	
	public String getName() {
		return name;
	}
	
	@Override
	public String toString() {
		return "@" + name;
	}

}
